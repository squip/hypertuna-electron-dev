{
  "name": "hypertuna-public-gateway",
  "version": "0.1.0",
  "description": "Public Hyperswarm gateway for Hypertuna relays",
  "type": "module",
  "main": "src/index.mjs",
  "scripts": {
    "dev": "node --env-file=.env.local src/index.mjs",
    "start": "node src/index.mjs",
    "lint": "eslint src --ext .js,.mjs",
    "test": "node --test",
    "blind-peer:status": "node ./bin/blind-peer-status.mjs",
    "web:dev": "npm run dev --prefix public-web",
    "web:build": "npm run build --prefix public-web",
    "web:preview": "npm run preview --prefix public-web"
  },
  "dependencies": {
    "blind-peer": "^2.9.2",
    "blind-peer-encodings": "^3.1.0",
    "hypercore-id-encoding": "^1.3.0",
    "express": "^4.19.2",
    "ws": "^8.18.0",
    "hyperswarm": "^4.14.0",
    "hypercore-crypto": "^3.5.0",
    "compact-encoding": "^2.12.0",
    "corestore": "^6.14.2",
    "hyperbee": "^2.13.4",
    "b4a": "^1.6.4",
    "prom-client": "^15.1.0",
    "pino": "^9.1.0",
    "pino-http": "^9.0.0",
    "yargs": "^17.7.2",
    "dotenv": "^16.4.5",
    "redis": "^4.6.13",
    "helmet": "^7.0.0",
    "express-rate-limit": "^7.2.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0"
  }
}
